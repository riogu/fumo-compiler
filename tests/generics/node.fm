struct Node|T| {
    let data: T;
    let next: Node|T|*;
    
    fn get_data() -> T {
        return data;
    }
    
    fn static make(value: T) -> Node|T| {
        return Node|T|{value, null};
    }
}

fn main() -> void {
    let head = Node|i32|::make(1);
    let second = Node|i32|::make(2);
    head.next = &second;
    
    let float_node = Node|f64|::make(1.5);
}
