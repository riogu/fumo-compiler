fn swap|T|(a: T*, b: T*) -> void {
    let temp: T = *a;
    *a = *b;
    *b = temp;
}

fn identity|T|(x: T) -> T {
    return x;
}

fn main() -> void {
    let x: i32 = 5;
    let y: i32 = 10;
    swap|i32|(&x, &y);
    
    let result = identity|f64|(2.5);
    let char_result = identity|char|('A');
}
